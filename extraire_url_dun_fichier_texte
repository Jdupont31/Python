import re
import os
import wget

os.chdir("C:/Users/Jo/Downloads/Resultat_Renault")

codesourcesouvert = open("../codeS.txt", "r")
a = codesourcesouvert.read()


pattern=re.compile('href=\"([^\s]+)\"')
result=pattern.findall(a)

print (len(result))
print (result);

for url in result:
    wget.download(url)
